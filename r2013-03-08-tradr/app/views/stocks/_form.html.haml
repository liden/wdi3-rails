.row
  .large-3.columns
    = form_for @stock, :remote => true do |f|
      = f.label :symbol
      = f.text_field :symbol
      = f.label :shares
      = f.text_field :shares
      = f.submit nil, :class => 'button radius tiny'
      = link_to('Cancel', '#', :id=>'cancel_stock', :class => 'button radius tiny alert')
  .large-9.columns